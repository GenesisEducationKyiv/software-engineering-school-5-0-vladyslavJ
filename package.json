{
  "name": "genesis-se-school-5-ts",
  "version": "1.0.0",
  "description": "Genesis SE School 2025",
  "main": "app.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "npm run migration:run && tsx --watch src/server.ts",
    "migration:run": "npm run build && node ./node_modules/typeorm/cli.js migration:run -d dist/infrastructure/config/dataSource.js",
    "migration:revert": "npm run build && node ./node_modules/typeorm/cli.js migration:revert -d dist/infrastructure/config/dataSource.js",
    "migration:show": "npm run build && node ./node_modules/typeorm/cli.js migration:show -d dist/infrastructure/config/dataSource.js",
    "migration:create": "ts-node src/shared/scripts/create-migration.script.ts",
    "db:create:test:docker": "cross-env NODE_ENV=test dotenv -e .env.test.docker -- ts-node src/shared/scripts/create-test-db.script.ts",
    "test:docker": "npm run db:create:test:docker && npm test",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "format": "prettier . --write",
    "test:unit": "jest --config tests/setup/jest-unit.config.ts",
    "test:int": "jest --config tests/setup/jest-integration.config.ts",
    "test:arch": "jest tests/architecture/dependency.test.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "node-cron": "^4.0.5",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "prom-client": "^15.1.3",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "testcontainers": "^11.0.3",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.5.0",
    "@playwright/test": "^1.53.0",
    "@types/cors": "^2.8.18",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/morgan": "^1.9.9",
    "@types/nock": "^11.1.0",
    "@types/node": "^22.15.18",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.1",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "nock": "^14.0.5",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsarch": "^5.4.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}